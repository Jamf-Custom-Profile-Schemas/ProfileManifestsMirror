{
    "title": "Cellular (com.apple.cellular)",
    "description": "The payload that configures cellular settings.",
    "properties": {
        "APNs": {
            "type": "array",
            "items": {
                "type": "object",
                "description": "A dictionary that contains details about an access point name (APN) configuration.",
                "properties": {
                    "Name": {
                        "type": "string",
                        "title": "Data APN name",
                        "description": "The name for this configuration."
                    },
                    "AuthenticationType": {
                        "type": "string",
                        "title": "Data APN authentication type",
                        "default": "PAP",
                        "description": "The authentication type for logging in.",
                        "enum": [
                            "CHAP",
                            "PAP"
                        ]
                    },
                    "Username": {
                        "type": "string",
                        "title": "Data APN user name",
                        "description": "The user name for the APN."
                    },
                    "Password": {
                        "type": "string",
                        "title": "Data APN password",
                        "description": "The user's password for the APN."
                    },
                    "ProxyServer": {
                        "type": "string",
                        "title": "Data APN proxy server",
                        "description": "The proxy server's address."
                    },
                    "ProxyPort": {
                        "type": "integer",
                        "title": "Data APN proxy port",
                        "description": "The proxy server's port number."
                    },
                    "DefaultProtocolMask": {
                        "type": "integer",
                        "title": "Data APN default IP versions",
                        "description": "The default Internet Protocol versions. Available in iOS 10.3 but no longer used in iOS 11 and later. Allowed values:\n\n- '1': IPv4\n- '2': IPv6\n- '3': Both",
                        "enum": [
                            1,
                            2,
                            3
                        ],
                        "options": {
                            "enum_titles": [
                                "IPv4",
                                "IPv6",
                                "IPv4 and IPv6"
                            ]
                        }
                    },
                    "AllowedProtocolMask": {
                        "type": "integer",
                        "title": "Data APN supported IP versions",
                        "description": "The Internet Protocol versions that the system supports. Available in iOS 10.3 and later. Allowed values:\n\n- '1': IPv4\n- '2': IPv6\n- '3': Both",
                        "enum": [
                            1,
                            2,
                            3
                        ],
                        "options": {
                            "enum_titles": [
                                "IPv4",
                                "IPv6",
                                "IPv4 and IPv6"
                            ]
                        }
                    },
                    "AllowedProtocolMaskInRoaming": {
                        "type": "integer",
                        "title": "Data APN supported roaming IP versions",
                        "description": "The Internet Protocol versions that the system supports while roaming. Available in iOS 10.3 and later. Allowed values:\n\n- '1': IPv4\n- '2': IPv6\n- '3': Both",
                        "enum": [
                            1,
                            2,
                            3
                        ],
                        "options": {
                            "enum_titles": [
                                "IPv4",
                                "IPv6",
                                "IPv4 and IPv6"
                            ]
                        }
                    },
                    "AllowedProtocolMaskInDomesticRoaming": {
                        "type": "integer",
                        "title": "Data APN supported domestic roaming IP versions",
