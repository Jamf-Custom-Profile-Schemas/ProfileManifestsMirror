                        "enum": [
                            1,
                            2,
                            3
                        ],
                        "options": {
                            "enum_titles": [
                                "IPv4",
                                "IPv6",
                                "IPv4 and IPv6"
                            ]
                        }
                    },
                    "EnableXLAT464": {
                        "type": "boolean",
                        "title": "Enable 464XLAT IPv6 and IPv4 Interoperability",
                        "default": false,
                        "description": "If 'true', the system enables XLAT464. Available in iOS 16 and later and watchOS 9 and later."
                    }
                }
            },
