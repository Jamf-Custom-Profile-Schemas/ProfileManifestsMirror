            "links": [
                {
                    "rel": "More information",
                    "href": "https://derflounder.wordpress.com/2017/06/29/activating-endnote-x8-using-management-profiles/"
                }
            ],
            "enum": [
                "1",
                "0"
            ],
            "options": {
                "enum_titles": [
                    "1: Accept and Suppress Dialog",
                    "0: Show Dialog to Users"
                ]
            },
            "property_order": 5
        },
        "AcceptedENX9EULA": {
            "type": "string",
            "title": "Accept EndNote X9 EULA",
            "default": "0",
